{
  "name": "@jam-comments/gatsby",
  "version": "0.2.16",
  "author": "Alex MacArthur <alex@macarthur.me> (https://macarthur.me)",
  "license": "GPL-2.0",
  "description": "The Gatsby plugin for Jam Comments.",
  "main": "gatsby-node.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./ui": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./*": {
      "import": "./gatsby-node.js",
      "require": "./gatsby-node.js"
    }
  },
  "typesVersions": {
    "*": {
      "ui": [
        "./dist/index.d.ts"
      ]
    }
  },
  "scripts": {
    "watch": "vite build --watch",
    "build": "vite build && tsc",
    "prettier": "prettier --write \"**/*.{md,js}\"",
    "install-peers": "install-peers -f"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexmacarthur/jam-comments-javascript.git"
  },
  "files": [
    "dist/",
    "src/",
    "package-lock.json"
  ],
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "comments",
    "jamstack"
  ],
  "peerDependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "bugs": {
    "url": "https://github.com/alexmacarthur/jam-comments-javascript/issues"
  },
  "homepage": "https://jamcomments.com",
  "dependencies": {
    "@jam-comments/utilities": "^0.1.16",
    "graphql-quest": "^1.0.2",
    "isomorphic-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@jam-comments/react": "^0.0.38",
    "@jam-comments/styles": "^0.0.19",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.38",
    "babel-loader": "^8.1.0",
    "dotenv": "^8.2.0",
    "install-peers-cli": "^2.2.0",
    "lint-staged": "^10.1.7",
    "prettier": "^1.19.1",
    "typescript": "^4.4.4",
    "vite": "^2.6.11"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "2b1cefb90c89a2f70099213881b09216f20e3dc2"
}
