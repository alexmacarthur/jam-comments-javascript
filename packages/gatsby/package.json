{
  "name": "@jam-comments/gatsby",
  "version": "1.0.4",
  "author": "Alex MacArthur <alex@macarthur.me> (https://macarthur.me)",
  "license": "GPL-2.0",
  "description": "The Gatsby plugin for Jam Comments.",
  "main": "gatsby-node.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./ui": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./*": {
      "import": "./gatsby-node.js",
      "require": "./gatsby-node.js"
    }
  },
  "typesVersions": {
    "*": {
      "ui": [
        "./dist/index.d.ts"
      ]
    }
  },
  "scripts": {
    "watch": "vite build --watch",
    "build": "vite build && tsc",
    "prettier": "prettier --write \"**/*.{md,js}\"",
    "install-peers": "install-peers -f",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexmacarthur/jam-comments-javascript.git"
  },
  "files": [
    "dist/",
    "src/",
    "package-lock.json"
  ],
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "comments",
    "jamstack"
  ],
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "bugs": {
    "url": "https://github.com/alexmacarthur/jam-comments-javascript/issues"
  },
  "homepage": "https://jamcomments.com",
  "dependencies": {
    "@jam-comments/client": "^1.0.2",
    "@jam-comments/server-utilities": "^1.0.2",
    "isomorphic-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "dotenv": "^16.0.3",
    "install-peers-cli": "^2.2.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "terser": "^5.15.1",
    "typescript": "^4.9.3",
    "vite": "^3.2.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "2b1cefb90c89a2f70099213881b09216f20e3dc2"
}
