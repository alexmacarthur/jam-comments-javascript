{
  "name": "@jam-comments/gatsby",
  "version": "0.0.1",
  "author": "Alex MacArthur <alex@macarthur.me> (https://macarthur.me)",
  "license": "GPL-2.0",
  "description": "The Gatsby plugin for Jam Comments.",
  "main": "gatsby-node.js",
  "module": "dist/jam-comments.min.js",
  "scripts": {
    "watch": "NODE_ENV=development JAM_COMMENTS_SERVICE_ENDPOINT=http://localhost:4000 npx rollup -c -w",
    "build": "NODE_ENV=production npx rollup -c",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --fix ./src/**/*.js",
    "prettier": "prettier --write \"**/*.{md,js}\"",
    "install-peers": "install-peers -f"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexmacarthur/jam-comments-javascript.git"
  },
  "files": [
    "dist/",
    "src/",
    "package-lock.json"
  ],
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "comments",
    "jamstack"
  ],
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "bugs": {
    "url": "https://github.com/alexmacarthur/jam-comments-javascript/issues"
  },
  "homepage": "https://jamcomments.com",
  "dependencies": {
    "@jam-comments/utilities": "0.0.1",
    "graphql-quest": "^1.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@jam-comments/react": "0.0.0",
    "@babel/core": "^7.9.0",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "babel-loader": "^8.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^2.7.0",
    "install-peers-cli": "^2.2.0",
    "lint-staged": "^10.1.7",
    "prettier": "^1.19.1",
    "pretty-quick": "^1.11.0",
    "rollup": "^2.33.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-postcss": "^3.1.1",
    "rollup-plugin-react-svg": "^3.0.3",
    "rollup-plugin-scss": "^2.4.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "828b7b723542ec00dbf4dca77a239d6637296e23"
}
