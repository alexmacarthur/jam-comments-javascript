{
  "name": "@jam-comments/next",
  "version": "5.1.1",
  "description": "A library for integrating JamComments into a Next.js application.",
  "type": "module",
  "main": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "types": "./dist/esm/index.d.ts"
    },
    "./server": {
      "import": "./dist/esm/server.js",
      "types": "./dist/esm/server.d.ts"
    }
  },
  "scripts": {
    "build": "rm -rf dist && npm run build:esm",
    "build:esm": "tsc --module es2020 --outDir dist/esm",
    "prepare": "npm run build",
    "format": "prettier --write src"
  },
  "files": [
    "./src",
    "./dist"
  ],
  "keywords": [],
  "author": "Alex MacArthur",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.14.11",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "prettier": "^3.3.3",
    "typescript": "^5.5.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "2b1cefb90c89a2f70099213881b09216f20e3dc2",
  "dependencies": {
    "@jam-comments/server-utilities": "^5.3.0",
    "next": "^14.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
